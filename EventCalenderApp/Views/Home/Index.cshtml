
@model System.Collections.Generic.IEnumerable<EventCalenderApp.Models.ViewModels.PublishedViewModels>

@{
    ViewBag.Title = "Home Page";
}
@foreach (var ss in Model)
{
    <div class="jumbotron">
        <h2>@ss.EventTitle</h2>
        <p ><span>Type : </span>@ss.EventTypeName</p>
        <p><span>Venue : </span>@ss.EventVenueName</p>
        <p><span>Time : </span>@ss.EventStartTime <span> to </span>@ss.EventEndTime</p>
        <div class="btn-lg btn btn-info">
            @Html.ActionLink("Register", "Account")
        </div>
    </div>
}


